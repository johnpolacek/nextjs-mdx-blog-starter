{
  "name": "nextjs-mdx-blog-starter",
  "description": "Starter for creating a Next.js powered blog with content authored in MDX.",
  "version": "0.2.3",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format-watch": "npm run format && onchange \"**/*.{js,jsx,json,md}\" -- prettier --write {{changed}}",
    "deploy": "git push origin main && vercel --prod"
  },
  "dependencies": {
    "@artsy/fresnel": "^1.3.1",
    "@emotion/react": "^11.1.4",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/runtime": "^1.6.22",
    "@next/mdx": "^10.0.5",
    "disqus-react": "^1.0.10",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "gray-matter": "^4.0.2",
    "next": "^10.0.5",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "theme-ui": "^0.3.5",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/theme-ui": "^0.3.7",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1"
  },
  "repository": "https://github.com/johnpolacek/nextjs-mdx-blog-starter",
  "license": "MIT"
}
